    "url": "https://github.com/manizm/cachinator/issues"
  },
  "homepage": "https://github.com/manizm/cachinator#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.7",
    "gts": "^4.0.0",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "typedoc": "^0.22.17",
    "typescript": "^4.5.4"
  }
